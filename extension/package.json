{
  "name": "tytler",
  "displayName": "tytler",
  "description": "Helper function to work with tytler",
  "version": "0.2.9",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/frbosquet/tytler"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tytler.replace-with-translation",
        "title": "Replace with translation",
        "category": "Tytler"
      },
      {
        "command": "tytler.replace-and-sync",
        "title": "Replace with translation and sync languages",
        "category": "Tytler"
      },
      {
        "command": "tytler.add-translation",
        "title": "Add a translation",
        "category": "Tytler"
      },
      {
        "command": "tytler.sync",
        "title": "Sync languages",
        "category": "Tytler"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.1.0",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
